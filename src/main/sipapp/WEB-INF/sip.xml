<?xml version="1.0" encoding="UTF-8"?>

<sip-app>
	<app-name>Sip Application</app-name>
	<display-name>SipSample</display-name>
	<description>SipSample</description>

	<servlet-selection>
		<main-servlet>
			SimpleSipServlet
		</main-servlet>
	</servlet-selection>

	<servlet>
		<servlet-name>SimpleSipServlet</servlet-name>
		<display-name>SimpleSipServlet</display-name>
		<description>Simple SIP servlet</description>
		<servlet-class>
			by.epolsoft.servlet.SimpleSipServlet
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<!--<context-param>-->
		<!--<param-name>bye.delay</param-name>-->
		<!--<param-value>60000</param-value>-->
	<!--</context-param>-->

	<listener>
		<listener-class>
			by.epolsoft.servlet.SimpleSipServlet
		</listener-class>
	</listener>
</sip-app>